<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='ontobrowser'>
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.user.theme.clean.Clean'/>
  <inherits name='com.google.gwt.resources.Resources' />
  <inherits name='org.hibernate.Hibernate'/>
  <inherits name='com.novartis.pcs.ontology.Ontology'/>
  <stylesheet src="../gwt-override.css"/>
  <entry-point class='com.novartis.pcs.ontology.webapp.client.OntoBrowser'/>

  <set-configuration-property name="CssResource.enableGss" value="true" />
  <set-configuration-property name="CssResource.conversionMode" value="strict" />
  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>
</module>
